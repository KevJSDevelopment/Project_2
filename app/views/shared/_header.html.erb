<nav class="navbar navbar-light" style="background-color: #1F2F39;">
    <div class="info">
        <a class="" href="/" style="font-size: 28px" role="button">
        <img src="data:image/jpeg;base64,", width= "90", height="45"></a>
        <div class="profile" style="padding-left: 10px;">
            <%if session[:user_id]%>
                <a class="btn btn-outline-info" href="/users/:id" role="button">Profile</a>
                <a class="btn btn-outline-secondary" href="/logout" role="button">Logout</a>
            <%else%>
                <a class="btn btn-outline-info" href="/login" role="button">Login</a>
                <a class="btn btn-outline-success" href="/users/new" role="button">Sign Up</a>
            <%end%>
        </div>
    </div>
  <div class="form-inline">
    <div style="padding-left: 10px;">
        <a class="btn btn-outline-light" href="/items" role="button"> Shop </a>
