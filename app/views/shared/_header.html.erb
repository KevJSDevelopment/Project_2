            <a class="btn btn-outline-light" href="/orders" role="button">My Orders</a>
             <a class="btn btn-outline-light" href="/wishlists" role="button">My Wishlists</a>
        <%end%>

        <div class= "search">
            <%= form_tag(items_path, method: :get) do %>
            <%= select_tag :category, options_from_collection_for_select(Category.all, "id", "name"),  include_blank: "All Categories", class: 'form-control'  %>
            <%= text_field_tag(:search, params[:search], class: 'form-control') %>
            <%= submit_tag ("Search"), class: 'form-control'%>
            <% end %>
        </div>
    </div>
  </div>
